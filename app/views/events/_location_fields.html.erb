<fieldset>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :street_address %>
	<%= f.text_field :street_address %>

	<%= f.label :city %>
	<%= f.text_field :city %>

	<%= f.label :state %>
	<%= f.text_field :state %>

	<%= f.label :zip %>
	<%= f.text_field :zip %>

	<%= f.label :cross_streets %>
	<%= f.text_field :cross_streets %>

	<%= f.label :website %>
	<%= f.text_field :website %>

	<%= f.hidden_field :_destroy %>
	<%= link_to "remove", '#', class: "remove_fields" %>

</fieldset>

